public class New {

	public static void main(String[] args) {
		String s="WELCOME";
		int half=s.length()/2;
		int sp= s.length()-1;
		for(int i=1;i<=s.length();i++) {
			for(int j=1;j<=sp;j++) 
				System.out.print(" ");
				for(int k=half;k<i;k++)
					System.out.print(s.charAt(k));
				System.out.println();
				sp--;
		}

	}

}
